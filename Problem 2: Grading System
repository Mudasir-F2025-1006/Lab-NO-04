  #include <iostream>
 using namespace std;
 int main() {
     int percentage;
     cout<< "Percentage:" << endl;
     cin >> percentage;
     switch (percentage/10) {
         case 9 : {
             cout << " A ";
             break;
         }
         case 8 : {
             cout << "B" ;
             break;
         }
         case 7 : {
             cout << "c";
             break;
         }
         case 6 : {
             cout << "D";
             break;
         }
         default : {
             cout  << "FailðŸ˜‚ " << endl;
         }
         
         
     }
     return 0;
     
 }
